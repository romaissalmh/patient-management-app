<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Doctor_app_tdm" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimaryVariant">@color/gris_fonce</item>
        <item name="colorOnPrimary">@color/gris_fonce</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/gris_clair</item>
        <item name="colorSecondaryVariant">@color/gris_clair</item>
        <item name="colorOnSecondary">@color/gris_clair</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->


    </style>

    <style name="Theme.Doctor_app_tdm.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Doctor_app_tdm.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Doctor_app_tdm.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>